<table class="table table-dark table-hover">
 <thead>
    <tr>
    <th scope="col">Item</th>
    <th scope="col">Producto</th>
    <th scope="col">Precio</th>
    <th scope="col">Foto</th>
    </tr>
</thead>
{{#each productos}}
<tbody>
    <tr>
    <th scope="row">{{this.id}}</th>
    <td>{{this.name}}</td>
    <td>{{this.precio}}</td>
    <td>{{this.thumbnail}}</td>
    </tr>

</tbody>
{{/each}}
</table>