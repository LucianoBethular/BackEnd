{{#if productos.length }}

<table class="table table-dark table-hover">
<thead>
    <tr>
    <th scope="col">Item</th>
    <th scope="col">Producto</th>
    <th scope="col">Precio</th>
    <th scope="col">Foto</th>
    </tr>
</thead>
{{#each productos}}
<tbody>
    <tr>
    <th scope="row">{{this.id}}</th>
    <td>{{this.producto}}</td>
    <td>{{this.precio}}</td>
    <td style="width: 50px, heigth:50px;"><img src="{{this.foto}}" alt="" style="width: 50px, heigth:50px;"></td>
    </tr>

</tbody>
{{/each}}
</table>

{{else}}
<h2>No hay productos disponibles</h2>

{{/if}}